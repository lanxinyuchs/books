<appdata target="alarm">
  <alarmtype
      name="FallbackOperationStartingSoon" 
      majorType="193"
      minorType="9175043"
      moClasses="UpgradePackage"
      specificProblem="A Fallback Operation will soon be started"
      eventType="OTHER"
      probableCause="164"
      isStateful="true"
      additionalText="The upgrade operation has not been confirmed yet. The ME will soon start fallback operation if the upgrade is not confirmed."
      defaultSeverity="WARNING">
    <registerOnNodes>
      <node type="BPU" />
      <node type="RadioNode" />
      <node type="RadioTNode" />
      <node type="RnNode" />
      <node type="VtfRadioNode" />
      <node type="5GRadioNode" />
      <node type="RCP-SIM" />
      <node type="RCS-DUS2" />
      <node type="RCS-T" />
    </registerOnNodes>
  </alarmtype>
  <alarmtype
      name="ScheduledBackupFailed" 
      majorType="193"
      minorType="9175044"
      moClasses="BrmBackupScheduler,BrmSingleEvent,BrmPerodicEvent,BrmCalendarBasedPerodicEvent"
      specificProblem="Scheduled Backup Failed"
      eventType="PROCESSINGERRORALARM"
      probableCause="348"
      isStateful="true"
      additionalText="The scheduled backup creation failed or an event could not be scheduled"
      defaultSeverity="WARNING">
    <registerOnNodes>
      <node type="ALL" />
    </registerOnNodes>
  </alarmtype>
  <alarmtype
      name="AutoExportBackupFailed" 
      majorType="193"
      minorType="9175045"
      moClasses="BrmBackupScheduler"
      specificProblem="Auto Export Backup Failed"
      eventType="OTHER"
      probableCause="418"
      isStateful="true"
      additionalText="Export failed. For more information check the progress report for the specific backup"
      defaultSeverity="WARNING">
    <registerOnNodes>
      <node type="ALL" />
    </registerOnNodes>
  </alarmtype>
  <alarmtype
      name="SoftwareManagementLostConnectiontoFileServer" 
      majorType="193"
      minorType="9175195"
      moClasses="UpgradePackage"
      specificProblem="Software Management Lost Connection to File Server"
      eventType="PROCESSINGERRORALARM"
      probableCause="307"
      isStateful="true"
      additionalText="Connectivity to the upgrade package file server is lost. Software download is not possible"
      defaultSeverity="WARNING">
    <registerOnNodes>
      <node type="BPU" />
      <node type="RadioNode" />
      <node type="RadioTNode" />
      <node type="RnNode" />
      <node type="VtfRadioNode" />
      <node type="5GRadioNode" />
      <node type="RCP-SIM" />
      <node type="RCS-DUS2" />
      <node type="RCS-T" />
    </registerOnNodes>
  </alarmtype>
</appdata>
