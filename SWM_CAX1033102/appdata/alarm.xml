<appdata target="alarm">
  <alarmtype
      name="FallbackOperationStartingSoon" 
      majorType="193"
      minorType="9175043"
      moClasses="UpgradePackage"
      specificProblem="A Fallback Operation will soon be started"
      eventType="OTHER"
      probableCause="164"
      isStateful="true"
      additionalText="The upgrade operation has not been confirmed yet. The ME will soon start fallback operation if the upgrade is not confirmed."
      defaultSeverity="WARNING"/>
  <alarmtype
      name="ScheduledBackupFailed" 
      majorType="193"
      minorType="9175044"
      moClasses="BrmBackupScheduler,BrmSingleEvent,BrmPerodicEvent,BrmCalendarBasedPerodicEvent"
      specificProblem="Scheduled Backup Failed"
      eventType="PROCESSINGERRORALARM"
      probableCause="348"
      isStateful="true"
      additionalText="The scheduled backup creation failed or an event could not be scheduled"
      defaultSeverity="WARNING"/>
  <alarmtype
      name="AutoExportBackupFailed" 
      majorType="193"
      minorType="9175045"
      moClasses="BrmBackupScheduler"
      specificProblem="Auto Export Backup Failed"
      eventType="OTHER"
      probableCause="418"
      isStateful="true"
      additionalText="Export failed. For more information check the progress report for the specific backup"
      defaultSeverity="WARNING"/>
</appdata>
