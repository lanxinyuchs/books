APP = bld.rcsErlApp( "aic", ["model"], cxaid="180", cxcid="131", )

bld.shlib(
   source="csrc/ericssonDiscoveryMIB.c",
   target="ericssonDiscoveryMIB",
   use=["netsnmp"],
   install_path = None
)

bld.install_as(bld.getLib(APP)+'/ericssonDiscoveryMIB.so', 'libericssonDiscoveryMIB.so')
bld.install_as(bld.getBldLib("aic")+'/ericssonDiscoveryMIB.so', 'libericssonDiscoveryMIB.so')
   

bld.install_files(bld.getPrv(APP)+'/bin',
   [
      "csrc/aicomplete.html",
      "csrc/boardrestore.html",
      "csrc/css_files.tar",
      "csrc/ea.html",
      "csrc/export.html",
      "csrc/help.html",
      "csrc/help_ea.html",
      "csrc/not_allowed.html"
   ])
