<appdata target="alarm">
  <alarmtype
      name="RollbackEscalationStarted"
      majorType="193"
      minorType="9175066"
      moClasses="SysM"
      specificProblem="Rollback Escalation Started"
      eventType="PROCESSINGERRORALARM"
      probableCause="348"
      isStateful="true"
      additionalText="Software error occured. Rollback to previous backup starts in one hour unless operator logs into ME."/>
  <alarmtype
      name="SystemGeneralProblem"
      majorType="193"
      minorType="9175067"
      moClasses="SystemFunctions"
      specificProblem="System General Problem"
      eventType="PROCESSINGERRORALARM"
      probableCause="348"
      isStateful="true"
      additionalText="This is a non-recoverable SW error, a HWfault not detectable by HW test, or a configuration fault."/>
</appdata>
