<appdata target="alarm">
  <alarmtype
      name="CertMCertificateNotAvailable" 
      majorType="193"
      minorType="9175052"
      moClasses="NodeCredential"
      specificProblem="Certificate Management a Valid Certificate is Not Available"
      eventType="OPERATIONALVIOLATION"
      probableCause="414"
      isStateful="true"
      additionalText="The certificate provided by the alarming object is expired, revoked, or unavailable, which may result in the failure of a secure using this certificate."
      defaultSeverity="CRITICAL"/>
  <alarmtype
      name="CertMCertificateToExpire" 
      majorType="193"
      minorType="9175053"
      moClasses="NodeCredential"
      specificProblem="Certificate Management the Certificate is to Expire"
      eventType="QUALITYOFSERVICEALARM"
      probableCause="351"
      isStateful="true"
      additionalText="The threshold before certificate expiration has been crossed, and the certificate should be renewed to prevent a secure service failure."
      defaultSeverity="WARNING"/>
  <alarmtype
      name="CertMAutomaticEnrollmentFailed" 
      majorType="193"
      minorType="9175054"
      moClasses="NodeCredential"
      specificProblem="Certificate Management Automatic Enrollment Failed"
      eventType="PROCESSINGERRORALARM"
      probableCause="307"
      isStateful="true"
      additionalText="Automatic enrollment or renewal failed to execute due to local misconfiguration or remote enrollment service denial."
      defaultSeverity="WARNING"/>
</appdata>
