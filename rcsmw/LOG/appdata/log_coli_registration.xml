<appdata target="coli_reg">
  <coli_erlang_reg
      cli_name = "ramlog"
      cli_path = "/log"
      cli_scope = "central"
      usage = "Arg1 Arg2"
      module = "logRamI" function = "coli_set_severity">
    <description>

Set log severity level for a RAM log

Arg1 = The name of the RAM log
Arg2 = The severity filter level: 0..9

    </description>
  </coli_erlang_reg>
</appdata>
