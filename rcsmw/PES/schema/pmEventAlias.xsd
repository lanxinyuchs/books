<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">


  <xs:element name="appdata" type="RcsPmeType"/>


  <xs:complexType name="RcsPmeType">
    <xs:sequence>
      <xs:element name="eventMapId"  type="xs:string"
		  minOccurs="1" maxOccurs="1" />
      <xs:element name="eventGroupAlias" type="EventGroupAliasType"
		  minOccurs="0" maxOccurs="unbounded"/>
      <xs:element name="eventTypeAlias" type="EventTypeAliasType"
		  minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
    <xs:attribute name="target" type="PmeType" use="required"/>
  </xs:complexType>


  <xs:simpleType name="PmeType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="pmEventAlias" />
    </xs:restriction>
  </xs:simpleType>

  <xs:complexType name="EventGroupAliasType">
    <xs:sequence>
      <xs:element name="eventGroupId"  type="xs:string"
		  minOccurs="1" maxOccurs="1" />
      <xs:element name="groupAlias"   type="xs:integer"
		  minOccurs="1" maxOccurs="1" />
    </xs:sequence>
  </xs:complexType>

  
  <xs:complexType name="EventTypeAliasType">
    <xs:sequence>
      <xs:element name="eventTypeId" type="xs:string"
		  minOccurs="1" maxOccurs="1" />
      <xs:element name="typeAlias" type="xs:integer"
		  minOccurs="1" maxOccurs="1" />
    </xs:sequence>
  </xs:complexType>

</xs:schema>
