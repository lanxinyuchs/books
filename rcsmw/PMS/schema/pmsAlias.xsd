<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">


  <xs:element name="appdata" type="RcsPmsType"/>


  <xs:complexType name="RcsPmsType">
    <xs:sequence>
      <xs:element name="counterMapId"  type="xs:string"
		  minOccurs="1" maxOccurs="1" />
      <xs:element name="pmGroupAlias" type="PmGroupAliasType"
		  minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
    <xs:attribute name="target" type="PmsType" use="required"/>
  </xs:complexType>


  <xs:simpleType name="PmsType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="pmsGroupAlias" />
    </xs:restriction>
  </xs:simpleType>

  <xs:complexType name="PmGroupAliasType">
    <xs:sequence>
      <xs:element name="pmGroupId"  type="xs:string"
		  minOccurs="1" maxOccurs="1" />
      <xs:element name="groupAlias"   type="xs:integer"
		  minOccurs="1" maxOccurs="1" />
      <xs:element name="measurementTypeAlias" type="MeasurementTypeAliasType"
		  minOccurs="0" maxOccurs="unbounded" />
    </xs:sequence>
  </xs:complexType>

  
  <xs:complexType name="MeasurementTypeAliasType">
    <xs:sequence>
      <xs:element name="measurementTypeId" type="xs:string"
		  minOccurs="1" maxOccurs="1" />
      <xs:element name="typeAlias" type="xs:integer"
		  minOccurs="1" maxOccurs="1" />
    </xs:sequence>
  </xs:complexType>

</xs:schema>
