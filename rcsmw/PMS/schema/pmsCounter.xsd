<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">


  <xs:element name="appdata" type="RcsPmsType"/>


  <xs:complexType name="RcsPmsType">
    <xs:sequence>
      <xs:element name="pmGroup" type="PmGroup"
		                 minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
    <xs:attribute name="target" type="PmsType" use="required"/>
  </xs:complexType>


  <xs:simpleType name="PmsType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="pmsCounter" />
    </xs:restriction>
  </xs:simpleType>



<!--
#  PmGroup
-->


  <xs:complexType name="PmGroup">
    <xs:sequence>
      <xs:element name="pmGroupId"           type="xs:string"/>
      <xs:element name="category"            type="xs:string"
	        	                     minOccurs="0" maxOccurs="1"/>
      <xs:element name="consistentData"      type="xs:boolean"
		                             minOccurs="0" maxOccurs="1"/>
      <xs:element name="generation"          type="xs:string"
		                             minOccurs="0" maxOccurs="1"/>
      <xs:element name="switchingTechnology" type="xs:string"
		                             minOccurs="0" maxOccurs="1"/>
      <xs:element name="validity"            type="xs:boolean"
		                             minOccurs="0" maxOccurs="1"/>
      <xs:element name="moClass"             type="MoClass"
		                             minOccurs="0" maxOccurs="1"/> 
      <xs:element name="description"         type="xs:string"
		                             minOccurs="0" maxOccurs="1"/>
      <xs:element name="pmGroupVersion"      type="xs:string"
		                             minOccurs="0" maxOccurs="1"/>
      <xs:element name="measurementType"     type="MeasurementType"
		  minOccurs="1" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>



  <xs:complexType name="MoClass">
    <xs:sequence>
      <xs:element name="moClassName" type="xs:string"/>
      <xs:element name="mimName"     type="xs:string"/>
      <xs:element name="mimVersion"  type="xs:string"/>
      <xs:element name="mimRelease"  type="xs:string"/>
    </xs:sequence>
  </xs:complexType>


  <xs:complexType name="MeasurementType">
    <xs:sequence>
      <xs:element name="measurementTypeId"  type="xs:string"/>
      <xs:element name="measurementName"    type="xs:string"/>
      <xs:element name="size"               type="xs:unsignedInt"/>
      <xs:element name="collectionMethod"   type="CollectionMethodEnum"/>
      <xs:element name="description"        type="xs:string"/>
      <xs:element name="condition"          type="xs:string"/>
      <xs:element name="aggregation"        type="AggregationEnum"/>
      <xs:element name="measurementStatus"  type="MeasurementStatusEnum"/>
      <xs:element name="measurementResult"  type="xs:string"
		                            minOccurs="0" maxOccurs="1"/>
      <xs:element name="multiplicity"       type="xs:integer"/>
      <xs:element name="initialValue"       type="xs:integer"
		                            minOccurs="0" maxOccurs="1"/>
      <xs:element name="resetAtGranPeriod"  type="xs:boolean"
		                            minOccurs="0" maxOccurs="1"/>
      <xs:element name="derSampleRate"      type="xs:integer"
		                            minOccurs="0" maxOccurs="1"/>
      <xs:element name="fmAlarmType"        type="xs:integer"
		                            minOccurs="0" maxOccurs="1"/>
      <xs:element name="thresholdDirection" type="xs:integer"
		                            minOccurs="0" maxOccurs="1"/>
      <xs:element name="isCompressed"       type="xs:boolean"
		                            minOccurs="0" maxOccurs="1"/>
    </xs:sequence>
  </xs:complexType>


  <xs:simpleType name="CollectionMethodEnum">
    <xs:restriction base="xs:string">
      <xs:enumeration value="CC" />
      <xs:enumeration value="GAUGE" />
      <xs:enumeration value="DER" />
      <xs:enumeration value="SI" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="AggregationEnum">
    <xs:restriction base="xs:string">
      <xs:enumeration value="SUM" />
      <xs:enumeration value="AVG" />
      <xs:enumeration value="MIN" />
      <xs:enumeration value="MAX" />
      <xs:enumeration value="LAST_UPDATE" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="MeasurementStatusEnum">
    <xs:restriction base="xs:string">
      <xs:enumeration value="USED" />
      <xs:enumeration value="DEPRECATED" />
      <xs:enumeration value="OBSOLETE" />
      <xs:enumeration value="PRELIMINARY" />
    </xs:restriction>
  </xs:simpleType>


</xs:schema>
