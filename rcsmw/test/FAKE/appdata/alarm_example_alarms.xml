<appdata target="alarm"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="https://rbs-rde-dev.rnd.ki.sw.ericsson.se/vobs/rcs/dev/RCP_CSX10179/RCS_CRX901266/COMSA/COMSA_CXA11418/schema/alarm.xsd">

  <!-- minor type 9240558 is 65536 x 140 + 65518 -->
  <alarmtype
      name="ResourceMonitorCocoaPowderContainerLowLevel"
      majorType="193"
      minorType="9240558"
      moClasses="TestClass1"
      specificProblem="Resource Monitor Cocoa Powder Container Low Level"
      eventType="EQUIPMENTALARM"
      probableCause="1"
      isStateful="true"
      additionalText="Cocoa powder refill required"/>

  <!-- minor type 9240559 is 65536 x 140 + 65519 -->
  <alarmtype
      name="ResourceMonitorCoffeeBeansContainerLowLevel"
      majorType="193"
      minorType="9240559"
      moClasses="TestClass1"
      specificProblem="Resource Monitor Coffee Beans Container Low Level"
      eventType="EQUIPMENTALARM"
      probableCause="1"
      isStateful="true"
      additionalText="Coffee beans refill required"/>

  <!-- minor type 9240564 is 65536 x 140 + 65524 -->
  <alarmtype
      name="IntegrityMonitorExcessiveVibrations"
      majorType="193"
      minorType="9240564"
      moClasses="TestClass1"
      specificProblem="Integrity Monitor Excessive Vibrations"
      eventType="PHYSICALVIOLATION"
      probableCause="1"
      isStateful="false"
      additionalText="Physical intrusion or earthquake"/>
</appdata>
