<appdata target="pmEventJob">
  <eventJob> 
    <eventJobId>PredefEventJob</eventJobId>
    <description>Testing predef jobs</description>
    <eventFilter>
      <filterName>SingleSelect</filterName>
      <filterValue>value</filterValue>
    </eventFilter>
    <eventFilter>
      <filterName>MultiSelect</filterName>
      <filterValue>value</filterValue>
    </eventFilter>
    <requestedJobState>ACTIVE</requestedJobState>
    <reportingPeriod>FIFTEEN_MIN</reportingPeriod>
    <jobControl>STARTSTOP</jobControl>
    <eventGroupRef>"ManagedElement=1,SystemFunctions=1,PmEventM=1,EventProducer=1,EventGroup=EventGrp1"</eventGroupRef>
    <eventGroupRef>"ManagedElement=1,SystemFunctions=1,PmEventM=1,EventProducer=1,EventGroup=EventGrp2"</eventGroupRef>
    <eventTypeRef>"ManagedElement=1,SystemFunctions=1,PmEventM=1,EventProducer=1,EventGroup=EventGrp1,EventType=EvType2"</eventTypeRef>
    <fileOutputEnabled>true</fileOutputEnabled>
    <fileCompressionType>GZIP</fileCompressionType>
    <streamOutputEnabled>true</streamOutputEnabled>
    <streamDestinationIpAddress>123.21.34.56</streamDestinationIpAddress>
    <streamDestinationPort>1234</streamDestinationPort>
    <streamCompressionType>GZIP</streamCompressionType>
    <eventProducer>first</eventProducer>
  </eventJob>

</appdata>


