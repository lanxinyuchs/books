<appdata target="pmEventProducer">
      <eventProducer> 
        <eventProducerId>first</eventProducerId>
        <streamingCapabilities>
          <streamCapabilitiesId>gzip</streamCapabilitiesId>
          <supportedCompressionTypes>GZIP</supportedCompressionTypes>
        </streamingCapabilities>
        <filePullCapabilities>
          <filePullCapabilitiesId>15min</filePullCapabilitiesId>
          <alignedReportingPeriod>true</alignedReportingPeriod>
          <finalROP>false</finalROP>
          <outputDirectory>eventRop</outputDirectory>
          <supportedCompressionTypes>GZIP</supportedCompressionTypes>
          <supportedReportingPeriods>FIFTEEN_MIN</supportedReportingPeriods>
        </filePullCapabilities>
        <eventCapabilities>
          <eventCapabilitiesId>firstEventCap</eventCapabilitiesId>
          <maxNoOfJobs>6</maxNoOfJobs>
        </eventCapabilities>
      </eventProducer>
      <eventProducer> 
        <eventProducerId>second</eventProducerId>
        <streamingCapabilities>
          <streamCapabilitiesId>gzip</streamCapabilitiesId>
          <supportedCompressionTypes>GZIP</supportedCompressionTypes>
        </streamingCapabilities>
        <filePullCapabilities>
          <filePullCapabilitiesId>quarter</filePullCapabilitiesId>
          <alignedReportingPeriod>true</alignedReportingPeriod>
          <finalROP>false</finalROP>
          <outputDirectory>eventRop</outputDirectory>
          <supportedCompressionTypes>GZIP</supportedCompressionTypes>
          <supportedReportingPeriods>FIFTEEN_MIN</supportedReportingPeriods>
        </filePullCapabilities>
        <eventCapabilities>
          <eventCapabilitiesId>secondEventCap</eventCapabilitiesId>
          <maxNoOfJobs>6</maxNoOfJobs>
        </eventCapabilities>
      </eventProducer>
      <eventProducer> 
        <eventProducerId>ift</eventProducerId>
        <streamingCapabilities>
          <streamCapabilitiesId>gzip</streamCapabilitiesId>
          <supportedCompressionTypes>GZIP</supportedCompressionTypes>
        </streamingCapabilities>
        <filePullCapabilities>
          <filePullCapabilitiesId>15min</filePullCapabilitiesId>
          <alignedReportingPeriod>true</alignedReportingPeriod>
          <finalROP>false</finalROP>
          <outputDirectory>iftEventRop</outputDirectory>
          <supportedCompressionTypes>GZIP</supportedCompressionTypes>
          <supportedReportingPeriods>FIFTEEN_MIN</supportedReportingPeriods>
        </filePullCapabilities>
        <eventCapabilities>
          <eventCapabilitiesId>iftEventCap</eventCapabilitiesId>
          <maxNoOfJobs>6</maxNoOfJobs>
        </eventCapabilities>
      </eventProducer>
</appdata>


