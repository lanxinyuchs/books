<appdata target="coli_reg">
    <coli_cri_reg 
     cli_name = "restart"
     cli_path = "/dummy/"
     usage = "restart command"
     subcommand = "restart">     
     <description>
      restart pri cold|warm
      restart eri cold|warm
     </description>
    </coli_cri_reg>
    <coli_authorization
     cli_name = "restart"
     cli_path = "/dummy/"
     authorization= "expert">
    </coli_authorization>

</appdata>


